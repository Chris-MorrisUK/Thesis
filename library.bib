publisher = {Elsevier B.V.},
title = {{Ontology-driven analysis of UML-based collaborative processes using OWL-DL and CPN}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0167642309000884 http://www.sciencedirect.com/science/article/pii/S0167642309000884},
