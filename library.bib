issn = {23521465},
journal = {Transportation Research Procedia},
keywords = {Seamless travel,Technical Enabler,interoperability framework,one-stop shop,ticketing},
pages = {3218--3227},
title = {{IT2Rail: Information Technologies for Shift to Rail}},
