file = {:/adf.bham.ac.uk/eps/prhome20/C/CXM210/Papers/1-s2.0-S004316480300214X-main.pdf:pdf},
isbn = {0043-1648},
issn = {00431648},
journal = {Wear},
keywords = {Kalman filter,Point mechanisms,Remote condition monitoring,Wear assessment},
number = {7-12},
pages = {1209--1220},
title = {{Wear assessment employing remote condition monitoring: A case study}},
volume = {255},
year = {2003}
}
