issn = {17433509},
journal = {WIT Transactions on the Built Environment},
keywords = {Enterprise asset management,Enterprise information systems,Linear asset management,Rail management,Railway infrastructure asset management,Track maintenance management},
pages = {105--112},
title = {{Prioritized Rail Corridor Asset Management}},
volume = {88},
year = {2006}
}
