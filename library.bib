journal = {Expert Systems with Applications},
keywords = {AHP,Context model,GIS,ITS,Multi-criteria decision making technique,Ontology,Personalization,Route planning system,Semantic web,User model},
number = {2 PART 1},
pages = {2250--2259},
publisher = {Elsevier Ltd},
title = {{Ontology based personalized route planning system using a multi-criteria decision making approach}},
