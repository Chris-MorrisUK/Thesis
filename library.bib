issn = {2168-2216},
journal = {IEEE Transactions on Systems, Man, and Cybernetics: Systems},
pages = {1--14},
title = {{Dynamic Delay Predictions for Large-Scale Railway Networks: Deep and Shallow Extreme Learning Machines Tuned via Thresholdout}},
year = {2017}
}
