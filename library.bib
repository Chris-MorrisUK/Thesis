issn = {10848045},
journal = {Journal of Network and Computer Applications},
keywords = {Intelligent Transportation Systems (ITS),Ontologies,VANETs,Vehicular Networks,Vehicular accidents},
number = {6},
pages = {1891--1900},
publisher = {Elsevier},
title = {{VEACON: A Vehicular Accident Ontology designed to improve safety on the roads}},
url = {http://dx.doi.org/10.1016/j.jnca.2012.07.013},
volume = {35},
year = {2012}
